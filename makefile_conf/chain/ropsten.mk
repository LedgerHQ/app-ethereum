APP_LOAD_PARAMS += --path "44'/60'"
TICKER = "ETH"
CHAIN_ID = 3
# Starkware integration
APP_LOAD_PARAMS += --path "2645'/579218131'"
DEFINES += HAVE_STARKWARE
# Keep for Starkware Ropsten tests
DEFINES += HAVE_TOKENS_EXTRA_LIST
